    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "asika/http": "^1.0",
        "guzzlehttp/psr7": "^1.3",
        "phyrexia/http": "^2.4",
        "ringcentral/psr7": "^1.2",
        "slim/slim": "^3.4",
        "wandu/http": "^1.6",
        "zendframework/zend-diactoros": "^1.3"
    },
    "autoload": {
        "psr-4": {
